<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.web.WebView?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="574.0" prefWidth="934.0"
            xmlns="http://javafx.com/javafx/11.0.1">
    <children>
        <MenuBar layoutX="2.0" layoutY="1.0" prefHeight="26.0" prefWidth="800.0" AnchorPane.leftAnchor="0.0"
                 AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Close"/>
                        <MenuItem mnemonicParsing="false" text="Open"/>
                        <MenuItem mnemonicParsing="false" onAction="#addAccountAction" text="Add account"/>
                        <MenuItem mnemonicParsing="false" onAction="#composeMessageAction" text="Compose Message"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#optionsAction" text="Options"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <TreeView fx:id="emailsTreeView" layoutY="27.0" prefHeight="548.0" prefWidth="200.0"
                  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="27.0"/>
        <TableView fx:id="emailsTableView" layoutX="200.0" layoutY="28.0" prefHeight="184.0" prefWidth="603.0"
                   AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="27.0">
            <columns>
                <TableColumn fx:id="senderCol" prefWidth="119.199951171875" text="Sender"/>
                <TableColumn fx:id="subjectCol" minWidth="0.0" prefWidth="200.79995727539062" text="Subject"/>
                <TableColumn fx:id="recipientCol" minWidth="0.0" prefWidth="168.79998779296875" text="Recipient"/>
                <TableColumn fx:id="sizeCol" minWidth="0.0" prefWidth="76.00006103515625" text="Size"/>
                <TableColumn fx:id="dateCol" minWidth="0.0" prefWidth="166.39996337890625" text="Date"/>
            </columns>
        </TableView>
        <WebView fx:id="emailWebView" layoutX="200.0" layoutY="212.0" prefHeight="362.0" prefWidth="603.0"
                 AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="0.0"
                 AnchorPane.topAnchor="212.0"/>
    </children>
</AnchorPane>
